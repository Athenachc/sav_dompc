<?xml version="1.0"?>
<launch>

	<node pkg="airo_control" type="airo_control_node" name="airo_control_node" output="screen">
        <rosparam command="load" file="$(find airo_control)/config/vicon/fsm_vicon.yaml" ns="fsm"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/thrust_model.yaml" ns="thrust_model"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/observer/ekf_observer_vicon.yaml" ns="ekf_observer"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/observer/rd3_observer_vicon.yaml" ns="rd3_observer"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/controller/mpc_vicon.yaml" ns="mpc"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/controller/backstepping_vicon.yaml" ns="backstepping"/>
		<rosparam command="load" file="$(find airo_control)/config/vicon/controller/slidingmode_vicon.yaml" ns="slidingmode"/>
	</node>

</launch>